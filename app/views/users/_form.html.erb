<%= form_with(model: user) do |form| %>
   <div>
    <%= form.label :name, "ユーザー名" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit(class:"table_submit") %>
  </div>
<% end %>
